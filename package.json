{
	"name": "web-template",
	"version": "1.1.0",
	"description": "Monsterpet Clickfarm",
	"main": "electron-app.js",
	"scripts": {
		"serve": "http-server ./build -c-1",
		"build": "gulp",
		"watch": "gulp watch",
		"dev": "concurrently npm:build npm:serve npm:watch --restart-tries -1",
		"electron": "electron electron-app.js",
		"electron-build": "electron-builder --win"
	},
	"author": "skeddles",
	"license": "UNLICENCED",
	"dependencies": {
		"async": "^3.2.0",
		"concurrently": "^6.2.1",
		"file-url": "^3.0.0",
		"gulp": "^4.0.2",
		"gulp-environments": "^1.0.1",
		"gulp-hb": "^8.0.0",
		"gulp-imagemin": "^7.1.0",
		"gulp-include": "^2.4.1",
		"gulp-rename": "^2.0.0",
		"gulp-sass": "^5.0.0",
		"gulp-sourcemaps": "^3.0.0",
		"gulp-uglify": "^3.0.2",
		"gulp-util": "^3.0.8",
		"http-server": "^14.0.0",
		"puppeteer": "^9.1.1",
		"sass": "^1.42.1",
		"through2": "^4.0.2",
		"uglify-es": "^3.3.9"
	},
	"devDependencies": {
		"electron": "^30.0.9",
		"electron-builder": "^24.13.3"
	},
	"build": {
		"appId": "monsterpet-clickfarm",
		"productName": "Monsterpet Clickfarm",
		"directories": {
			"output": "executables"
		},
		"win": {
			"target": "portable"
		},
		"files": [
			"build/**/*",
			"electron-app.js",
			"package.json"
		]
	}
}
